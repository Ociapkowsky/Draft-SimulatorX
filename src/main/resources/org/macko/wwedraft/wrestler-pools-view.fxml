<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<SplitPane dividerPositions="0.33, 0.67" style="-fx-background-color: transparent;"  xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.macko.wwedraft.WrestlerPoolsViewController">
    <items>
        <VBox style="-fx-background-color: transparent;" spacing="5.0" alignment="CENTER">
            <children>
                <Label styleClass="label-header" text="%pools.header.me_mc" /> <ListView fx:id="mainEventMidCardListView" VBox.vgrow="ALWAYS" />
            </children>
            <padding>
                <Insets left="5.0" right="5.0" />
            </padding>
        </VBox>

        <VBox style="-fx-background-color: transparent;" spacing="5.0" alignment="CENTER">
            <children>
                <Label styleClass="label-header" text="%pools.header.lc_j" /> <ListView fx:id="lowCardJobbersListView" VBox.vgrow="ALWAYS" />
            </children>
            <padding>
                <Insets left="5.0" right="5.0" />
            </padding>
        </VBox>

        <VBox style="-fx-background-color: transparent;" spacing="5.0" alignment="CENTER">
            <children>
                <Label styleClass="label-header" text="%pools.header.tt" /> <ListView fx:id="tagTeamsListView" VBox.vgrow="ALWAYS" />
            </children>
            <padding>
                <Insets left="5.0" right="5.0" />
            </padding>
        </VBox>
    </items>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</SplitPane>